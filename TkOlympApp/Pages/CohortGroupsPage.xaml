<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:TkOlympApp.Pages"
             xmlns:svc="clr-namespace:TkOlympApp.Services"
             x:Class="TkOlympApp.Pages.CohortGroupsPage"
             Title="{svc:Translate CohortGroups_Title}">

    <RefreshView x:Name="PageRefresh" Refreshing="OnRefresh">
        <ScrollView>
            <VerticalStackLayout Padding="20" Spacing="12">
                <VerticalStackLayout x:Name="GroupsStack" Spacing="10" />
            </VerticalStackLayout>
        </ScrollView>
    </RefreshView>
</ContentPage>
